{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.js" }
    ],
    "env": {
      "DB_USER": "@db_user",
      "DB_HOST": "@db_host",
      "DB_DATABASE": "@db_database",
      "DB_PASSWORD": "@db_password",
      "DB_PORT": "@db_port"
    }
  }
  